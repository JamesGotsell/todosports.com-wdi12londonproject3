<p id="notice"><%= notice %></p>

<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Url</th>
      <th>Title</th>
      <th>Description</th>
      <th>Start time</th>
      <th>Stop time</th>
      <th>Venue</th>
      <th>Venue url</th>
      <th>Venue name</th>
      <th>Venue display</th>
      <th>City name</th>
      <th>Region name</th>
      <th>Region abbr</th>
      <th>Country name</th>
      <th>All day</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Geocode type</th>
      <th>Trackback count</th>
      <th>Calendar count</th>
      <th>Comment count</th>
      <th>Link count</th>
      <th>Created</th>
      <th>Owner</th>
      <th>Modified</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.id %></td>
        <td><%= event.url %></td>
        <td><%= event.title %></td>
        <td><%= event.description %></td>
        <td><%= event.start_time %></td>
        <td><%= event.stop_time %></td>
        <td><%= event.venue_id %></td>
        <td><%= event.venue_url %></td>
        <td><%= event.venue_name %></td>
        <td><%= event.venue_display %></td>
        <td><%= event.city_name %></td>
        <td><%= event.region_name %></td>
        <td><%= event.region_abbr %></td>
        <td><%= event.country_name %></td>
        <td><%= event.all_day %></td>
        <td><%= event.latitude %></td>
        <td><%= event.longitude %></td>
        <td><%= event.geocode_type %></td>
        <td><%= event.trackback_count %></td>
        <td><%= event.calendar_count %></td>
        <td><%= event.comment_count %></td>
        <td><%= event.link_count %></td>
        <td><%= event.created %></td>
        <td><%= event.owner %></td>
        <td><%= event.modified %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
